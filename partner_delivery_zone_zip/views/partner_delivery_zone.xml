<odoo>
    <record model="ir.ui.view" id="partner_delivery_zone_form_view">
        <field
            name="name"
        >partner.delivery.zone.form (in partner_delivery_zone_zip)</field>
        <field name="model">partner.delivery.zone</field>
        <field
            name="inherit_id"
            ref="partner_delivery_zone.view_partner_delivery_zone_form"
        />
        <field name="arch" type="xml">
            <field name="name" position="after">
                <field name="zip_ids">
                    <tree editable="bottom">
                        <field name="name" />
                    </tree>
                </field>
            </field>
        </field>
    </record>
</odoo>
